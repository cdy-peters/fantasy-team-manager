<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" maxWidth="250.0" prefHeight="400.0" prefWidth="250.0"
   xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1"
   fx:controller="com.example.client.controllers.LeaderBoardController">
   <children>
      <TableView fx:id="leaderboardTable">
         <columns>
            <TableColumn fx:id="rankColumn" prefWidth="80.0" text="Rank #" />
            <TableColumn fx:id="playerColumn" prefWidth="80.0" text="Player" />
            <TableColumn fx:id="scoreColumn" prefWidth="80.0" text="Score" />
         </columns>
      </TableView>
   </children>
</VBox>