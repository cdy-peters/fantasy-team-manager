<?xml version="1.0" encoding="UTF-8"?>

<?import com.example.client.components.Navbar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>

<VBox prefHeight="400.0" prefWidth="640.0" spacing="10" xmlns="http://javafx.com/javafx/22"
   xmlns:fx="http://javafx.com/fxml/1"
   fx:controller="com.example.client.controllers.LeaderBoardController">
   <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0"
         VBox.vgrow="ALWAYS">
         <Navbar>
            <padding>
               <Insets bottom="10.0" left="5.0" right="5.0" top="5.0" />
            </padding>
         </Navbar>
         <VBox layoutX="1.0" layoutY="40">
            <TableView fx:id="leaderboardTable" prefHeight="360" prefWidth="640.0">
               <columns>
                  <TableColumn fx:id="positionColumn" prefWidth="75.0" text="Rank #" />
                  <TableColumn fx:id="playerColumn" prefWidth="75.0" text="Player" />
                  <TableColumn fx:id="scoreColumn" prefWidth="75.0" text="Score" />
               </columns>
            </TableView>
         </VBox>
      </AnchorPane>
   </children>
</VBox>
